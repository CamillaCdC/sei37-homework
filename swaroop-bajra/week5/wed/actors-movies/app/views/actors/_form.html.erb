<%= form_for @actor do |f| %>

  <%= f.label :name %><br>
  <%= f.text_field :name %><br>

  <%= f.label :dob, 'DOB' %><br>
  <%= f.text_field :dob %><br>

  <%= f.label :nationality %><br>
  <%= f.text_field :nationality %><br>


  <%= f.label :bio, 'BIO' %><br>
  <%= f.text_field :bio %><br>

  <%= f.label :image, 'Image URL' %><br>
  <%= f.text_field :image %><br>

  <br>
  <%= f.submit %>
<% end %>
